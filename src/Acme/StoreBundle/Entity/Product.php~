<?php

namespace Acme\StoreBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 */
class Product {

    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue
     */
    protected $id;
    /**
     *
     * @ORM\Column(type="string", length="100");
     */
    protected $name;
    /**
     * @ORM\Column(type="decimal", scale="2")
     */
    protected $price;
    /**
     * @ORM\Column(type="text")
     */
    protected $description;
}